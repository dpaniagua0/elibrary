<!DOCTYPE properties SYSTEM 'http://java.sun.com/dtd/properties.dtd'>

<properties>
  <entry key="series.query.findBookSeriesOfBook">
    <![CDATA[
     SELECT s.id, s.name
     FROM Libro l, Serie s 
     WHERE l.id = :id_libro 
     AND s.id = l.id_serie
    ]]>
  </entry>
</properties>
